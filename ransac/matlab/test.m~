




lT = 1;
lF = 5;

lo = 0;
ll = lo;

for jj = 1:100

    mT = poissrnd(lT);
    mF = poissrnd(lF);
    
m = mT+mF

s = 0;
for ii = 0:m
    
    s = s+ poisson(lT,ii)*poisson(lF, m-ii);
    
end

ln = log(s/poisson(lF,m));

ll = ln+ll
end


1 - 1/(1+exp(ll))



function p = poisson(lambda, k)

p = exp(-lambda)*lambda^k/factorial(k);

end









